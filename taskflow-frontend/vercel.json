{
  "version": 3,
  "framework": "angular",
  "builds": [
    {
      "src": "dist/taskflow-frontend/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/taskflow-frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/taskflow-frontend/server.mjs"
    }
  ]
}
